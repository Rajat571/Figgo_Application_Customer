                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_26sdp"
                    android:layout_gravity="end"
                    android:layout_marginEnd="@dimen/_12sdp"
                    android:layout_marginTop="@dimen/_41sdp"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="@dimen/_14sdp"
                        android:layout_height="@dimen/_12sdp"
                        android:layout_marginEnd="@dimen/_9sdp"
                        android:src="@drawable/ic_baseline_location_on_24"
                        android:layout_gravity="end"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="sans-serif-black"
                        android:text="location"
                        android:layout_gravity="end"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_12sdp" />

                </LinearLayout>





            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_26sdp"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/_72sdp"
                android:layout_marginEnd="@dimen/_9sdp"
                android:layout_marginStart="@dimen/_9sdp"
                android:background="@drawable/btn_left_corner"
                android:orientation="horizontal">

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:inputType="text"
                    android:layout_marginStart="@dimen/_12sdp"
                    android:textSize="@dimen/_12sdp"
                    android:layout_gravity="center"
                    android:background="@null"
                    android:hint="3rd Hold"
                    android:textColor="@color/black" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_26sdp"
                    android:layout_gravity="end"
                    android:layout_marginEnd="@dimen/_12sdp"
                    android:layout_marginTop="@dimen/_41sdp"
                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="@dimen/_14sdp"
                        android:layout_height="@dimen/_12sdp"
                        android:layout_marginEnd="@dimen/_9sdp"
                        android:src="@drawable/ic_baseline_location_on_24"
                        android:layout_gravity="end"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="sans-serif-black"
                        android:text="location"
                        android:layout_gravity="end"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_12sdp" />

                </LinearLayout>

            </LinearLayout>


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="@dimen/_12sdp"
               android:layout_gravity="center"
                android:layout_marginStart="@dimen/_12sdp"
                android:layout_marginTop="@dimen/_99sdp"
                android:fontFamily="sans-serif-black"
                android:textColor="@color/black"
                android:text="Add"/>
















            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_25sdp"
                android:layout_gravity="bottom"
                android:background="@color/colornavyblue"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_12sdp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:text="Submit"/>

