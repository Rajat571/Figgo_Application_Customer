<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@color/colorlightblue"
    tools:context=".Fragments.Shared_Cab_Fragment.RoundAndTourFragment">

    <include layout="@layout/headerbackwithnotification"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
       android:layout_marginTop="-26dp"
        android:orientation="horizontal">

        <androidx.cardview.widget.CardView
            android:layout_width="@dimen/_65sdp"
            android:layout_height="@dimen/_45sdp"
            app:cardCornerRadius="@dimen/_6sdp"
            app:cardMaxElevation="@dimen/_6sdp"
            android:layout_marginStart="@dimen/_1sdp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@drawable/arrow_right"
                    android:background="@color/coloryellow"
                    android:scaleType="fitXY" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="one way"
                    android:fontFamily="sans-serif-black"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:background="@color/colorcoffie"/>

            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="@dimen/_65sdp"
            android:layout_height="@dimen/_45sdp"
            app:cardCornerRadius="@dimen/_6sdp"
            android:layout_marginStart="@dimen/_11sdp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:src="@drawable/arrow_right"
                    android:scaleType="fitXY"
                    android:background="@color/white"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Round/Tour"
                    android:fontFamily="sans-serif-black"
                    android:textColor="@color/black"
                    android:gravity="center"
                    android:background="@color/colorcoffie"/>

            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
