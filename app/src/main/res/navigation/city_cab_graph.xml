<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/city_cab_graph"
    app:startDestination="@id/advance">


    <fragment
        android:id="@+id/advance"
        android:name="com.figgo.customer.Fragments.Advance_cityCab"
        android:label="fragment_advance_city_cab"
        tools:layout="@layout/fragment_advance_city_cab" >
        <action
            android:id="@+id/action_advance_to_vehicleBookActivity"
            app:destination="@id/vehicleBookActivity" />
    </fragment>

    <fragment
        android:id="@+id/current"
        android:name="com.figgo.customer.Fragments.Current_cityCab"
        android:label="fragment_current_city_cab"
        tools:layout="@layout/fragment_current_city_cab" >
        <action
            android:id="@+id/action_current_to_oneWay_Km_CountActivity"
            app:destination="@id/oneWay_Km_CountActivity" />
    </fragment>


   <fragment
        android:id="@+id/packagee"
        android:name="com.figgo.customer.Fragments.Package_cityCab"
        android:label="fragment_package_city_cab"
        tools:layout="@layout/package_work_progress" />

    <activity
        android:id="@+id/oneWay_Km_CountActivity"
        android:name="com.figgo.customer.UI.OneWay_Km_CountActivity"
        android:label="activity_one_way_km_count"
        tools:layout="@layout/activity_one_way_km_count" />
    <activity
        android:id="@+id/vehicleBookActivity"
        android:name="com.figgo.customer.UI.Current_Cab_DetailsActivity"
        android:label="vehiclebookactivity"
        tools:layout="@layout/current_cab_details_activity" />


</navigation>