<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/share_cab_graph"
    app:startDestination="@id/shareCab_Fragment">

    <fragment
        android:id="@+id/shareCab_Fragment"
        android:name="com.pearlorganisation.figgo.UI.Fragments.Shared_Cab_Fragment.ShareCab_Fragment"
        android:label="fragment_share_cab_"
        tools:layout="@layout/fragment_share_cab_" >
        <action
            android:id="@+id/action_shareCab_Fragment_to_liveSharecab_Fragment"
            app:destination="@id/liveSharecab_Fragment" />
        <action
            android:id="@+id/action_shareCab_Fragment_to_advanceSharecab_Fragment"
            app:destination="@id/advanceSharecab_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/liveSharecab_Fragment"
        android:name="com.pearlorganisation.figgo.UI.Fragments.Shared_Cab_Fragment.LiveSharecab_Fragment"
        android:label="fragment_live_sharecab_"
        tools:layout="@layout/fragment_live_sharecab_" >
        <action
            android:id="@+id/action_liveSharecab_Fragment_to_shareCab_Fragment"
            app:destination="@id/shareCab_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/advanceSharecab_Fragment"
        android:name="com.pearlorganisation.figgo.UI.Fragments.Shared_Cab_Fragment.AdvanceSharecab_Fragment"
        android:label="fragment_advance_sharecab_"
        tools:layout="@layout/fragment_advance_sharecab_" >
        <action
            android:id="@+id/action_advanceSharecab_Fragment_to_shareCab_Fragment"
            app:destination="@id/shareCab_Fragment" />
    </fragment>
</navigation>